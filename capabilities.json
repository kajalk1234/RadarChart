                "show": {
                    "displayName": "Show",
                    "displayNameKey": "",
                    "type": {
                        "bool": true
                    }
                },
                "axisColor": {
                    "displayName": "Color",
                    "description": "Change the color of axis",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "strokeWidth": {
                    "displayName": "Stroke width",
                    "description": "Value must be less than 5",
                    "type": {
                        "numeric": true
                    }
