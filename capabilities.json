                    }
                },
                "fontSize": {
                    "displayName": "Font size",
                    "description": "Change the font size of legend",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                },
                "legendColor": {
                    "displayName": "Font color",
                    "description": "Change the color of legend",
