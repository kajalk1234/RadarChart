    "drilldown": {
        "roles": [
            "category"
        ]
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "min": 0,
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    },
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 20000
                        }
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "measure"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": []
    },
    "sorting": {
        "default": {   }
    }
}
