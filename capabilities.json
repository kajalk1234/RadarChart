                {
                    "numeric": true
                }
            ]
        }
    ],
    "supportsLandingPage": true,
    "objects": {
        "plotOptions": {
            "displayName": "Radar plot",
            "description": "Display radar plot options",
            "properties": {
                "interpolation": {
                    "displayName": "Interpolation",
                    "description": "Configure the curve interpolation method",
                    "type": {
                        "enumeration": [
                            {
                                "value": "curveLinearClosed",
                                "displayName": "Linear"
                            },
                            {
                                "value": "curveCardinalClosed",
                                "displayName": "Cardinal closed"
                            },
                            {
                                "value": "curveBundle",
                                "displayName": "Bundle"
                            },
                            {
                                "value": "curveBasisClosed",
                                "displayName": "Basis closed"
                            },
                            {
                                "value": "curveNatural",
                                "displayName": "Natural"
                            }
                        ]
                    }
                },
                "strokeWidth": {
                    "displayName": "Stroke width",
                    "description": "Value must be less than or equal to 5",
                    "type": {
                        "numeric": true
                    }
                },
                "transparency": {
                    "displayName": "Transparency",
                    "description": "Set transparency for the plot color",
                    "type": {
                        "numeric": true
                    }
                },
                "circleRadius": {
                    "displayName": "Data point radius",
                    "description": "Value must be less than or equal to 5",
                    "type": {
                        "numeric": true
                    }
                },
                "showDataPoints": {
                    "displayName": "Data points",
                    "description": "Display data points on the plot",
                    "type": {
                        "bool": true
                    }
                },
                "showValuesDataPoints": {
                    "displayName": "Data values",
                    "description": "Display data values on the plot",
                    "type": {
                        "bool": true
                    }
                },
                "showAllValuesDataPoints": {
                    "displayName": "Display all",
                    "description": "Display all data values on the plot",
                    "type": {
                        "bool": true
                    }
                },
                "dataPointColor": {
                    "displayName": "Data point color",
                    "description": "Change the color of the data points",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "plotValueColor": {
                    "displayName": "Data value color",
                    "description": "Change the color of the data values",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        },
        "plotColors": {
            "displayName": "Plot colors",
            "description": "Display plot colors options",
            "properties": {
                "plotColor": {
                    "displayName": "Category Color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        },
        "labelOptions":{
            "displayName": "Label",
            "description": "Display label options",
            "properties":{
                "fontSize": {
                    "displayName": "Font size",
                    "description": "Font size for the labels",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                },
                "fontFamily": {
                    "displayName": "Font family",
                    "description": "Change the font family of labels",
                    "type": {
                        "formatting": {
                            "fontFamily": true
