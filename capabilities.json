                        "enumeration": [
                            {
                                "value": "Top",
                                "displayName": "Top"
                            },
                            {
                                "value": "Bottom",
                                "displayName": "Bottom"
                            },
                            {
                                "value": "Left",
                                "displayName": "Left"
                            },
                            {
                                "value": "Right",
                                "displayName": "Right"
                            },
                            {
                                "value": "LeftCenter",
                                "displayName": "Left center"
                            },
                            {
                                "value": "RightCenter",
                                "displayName": "Right centre"
                            },
                            {
                                "value": "TopCenter",
                                "displayName": "Top center"
                            },
                            {
                                "value": "BottomCenter",
                                "displayName": "Bottom center"
